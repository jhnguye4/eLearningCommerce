<p id="notice"><%= notice %></p>

<h1>Admins</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone Number</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.name %></td>
        <td><%= admin.email %></td>
        <td><%= admin.phone_num %></td>
        <td><%= admin.addr %></td>
        <td><%= link_to 'Show', admin %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back', welcome_path %>
